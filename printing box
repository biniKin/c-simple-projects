#include <iostream>
using namespace std;

int main(){
    for(int i = 0; i < 10; i++){
        switch(i){
            case 1: for(int j = 10; j > 0; j--){
                cout << "*";
            }
            cout << endl;
            break;
            case 2: for(int j = 1; j > 0; j--){
                cout << "*";
            }
            for(int space = 9; space > 0; space --){
                cout << " ";
